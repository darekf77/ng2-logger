{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "test-pwa": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "tmp-src",
      "prefix": "app",
      "architect": {
        "build": {
          "preserveSymlinks": true,
          "showCircularDependencies": false,
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/test-pwa",
            "index": "tmp-src/index.html",
            "main": "tmp-src/main.ts",
            "polyfills": "tmp-src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": false,
            "assets": [
              "tmp-src/favicon.ico",
              "tmp-src/assets",
              "tmp-src/manifest.webmanifest"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "tmp-src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [],
              "showCircularDependencies": false,
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ],
              "serviceWorker": true,
              "ngswConfigPath": "ngsw-config.json"
            }
          }
        },
        "serve": {
          "showCircularDependencies": false,
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "test-pwa:build",
            "aot": false
          },
          "configurations": {
            "production": {
              "browserTarget": "test-pwa:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "test-pwa:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "tmp-src/test.ts",
            "polyfills": "tmp-src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "tmp-src/favicon.ico",
              "tmp-src/assets",
              "tmp-src/manifest.webmanifest"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "tmp-src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "test-pwa:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "test-pwa:serve:production"
            }
          }
        }
      }
    },
    "ng2-logger": {
      "projectType": "library",
      "root": "tmp-projects-for-dist/ng2-logger",
      "sourceRoot": "tmp-projects-for-dist/ng2-logger/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "tmp-projects-for-dist/ng2-logger/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "tmp-projects-for-dist/ng2-logger/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "tmp-projects-for-dist/ng2-logger/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "tmp-projects-for-dist/ng2-logger/src/test.ts",
            "tsConfig": "tmp-projects-for-dist/ng2-logger/tsconfig.spec.json",
            "karmaConfig": "tmp-projects-for-dist/ng2-logger/karma.conf.js"
          }
        }
      }
    }
  },
  "defaultProject": "test-pwa"
}
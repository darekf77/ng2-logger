{
  "name": "ng2-logger",
  "version": "2.0.36",
  "description": "isomorphic logger for browser/server in typescript",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/darekf77/ng2-logger.git"
  },
  "tnp": {
    "type": "isomorphic-lib",
    "resources": [
      "README.md",
      "screen.png",
      "server.png"
    ],
    "overrided": {
      "includeOnly": [
        "chalk",
        "json-stringify-safe",
        "json5",
        "randomcolor"
      ]
    }
  },
  "browser": {
    "process": false
  },
  "keywords": [
    "isomorphic",
    "logger",
    "log",
    "typescript"
  ],
  "author": {
    "name": "Dariusz Filipiak"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/darekf77/ng2-logger/issues"
  },
  "homepage": "https://github.com/darekf77/ng2-logger#readme",
  "readmeFilename": "README.md",
  "devDependencies": {
    "@types/node": "9.3.0",
    "reflect-metadata": "^0.1.10",
    "tslib": "^1.8.1"
  },
  "dependencies": {
    "chalk": "^2.3.2",
    "json-stringify-safe": "^5.0.1",
    "json5": "^0.5.1",
    "randomcolor": "^0.5.3"
  }
}

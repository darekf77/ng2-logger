{
  "name": "ng2-logger",
  "version": "2.0.38",
  "description": "isomorphic logger for browser/server in typescript",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/darekf77/ng2-logger.git"
  },
  "tnp": {
    "type": "isomorphic-lib",
    "resources": [
      "README.md",
      "screen.png",
      "server.png"
    ],
    "overrided": {
      "includeOnly": [
        "chalk",
        "json-stringify-safe",
        "json5",
        "randomcolor"
      ]
    }
  },
  "browser": {
    "process": false
  },
  "keywords": [
    "isomorphic",
    "logger",
    "logging",
    "logowanie",
    "log",
    "typescript"
  ],
  "author": {
    "name": "Dariusz Filipiak"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/darekf77/ng2-logger/issues"
  },
  "homepage": "https://github.com/darekf77/ng2-logger#readme",
  "readmeFilename": "README.md"
}

{
  "tnp": {
    "resources": [
      "README.md",
      "screen.png",
      "server.png"
    ],
    "type": "isomorphic-lib",
    "overrided": {
      "includeAsDev": [
        "morphi",
        "@angular.*",
        "webpack",
        "webpack-dev-server",
        "html-webpack-plugin",
        "ts-loader",
        "babel.*",
        "mocha",
        "@types/mocha",
        "chai",
        "ts-sinon"
      ],
      "includeOnly": [
        "chalk",
        "json-stringify-safe",
        "json5",
        "randomcolor",
        "enum-values"
      ],
      "dependencies": {
        "enum-values": "^1.2.1"
      },
      "ignoreDepsPattern": []
    }
  },
  "name": "ng2-logger",
  "version": "2.0.48",
  "description": "isomorphic logger for browser/server in typescript",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/darekf77/ng2-logger.git"
  },
  "browser": {
    "process": false
  },
  "keywords": [
    "isomorphic",
    "logger",
    "logging",
    "logowanie",
    "log",
    "typescript"
  ],
  "author": {
    "name": "Dariusz Filipiak"
  },
  "bugs": {
    "url": "https://github.com/darekf77/ng2-logger/issues"
  },
  "homepage": "https://github.com/darekf77/ng2-logger#readme",
  "readmeFilename": "README.md",
  "husky": {
    "hooks": {
      "pre-push": "tnp deps:show:if:standalone"
    }
  },
  "dependencies": {
    "chalk": "^2.3.2",
    "enum-values": "^1.2.1",
    "json-stringify-safe": "^5.0.1",
    "json5": "^0.5.1",
    "randomcolor": "^0.5.3"
  },
  "devDependencies": {
    "@angular-devkit/build-optimizer": "^0.3.1",
    "@angular-devkit/schematics": "^0.0.42",
    "@angular/animations": "5.2.0",
    "@angular/cdk": "^5.2.5",
    "@angular/cli": "~1.6.8",
    "@angular/common": "5.2.0",
    "@angular/compiler": "5.2.0",
    "@angular/compiler-cli": "~5.1.0",
    "@angular/core": "5.2.0",
    "@angular/forms": "5.2.0",
    "@angular/http": "5.2.0",
    "@angular/language-service": "~5.1.0",
    "@angular/material": "^5.2.5",
    "@angular/platform-browser": "5.2.0",
    "@angular/platform-browser-dynamic": "5.2.0",
    "@angular/router": "5.2.0",
    "@ngtools/webpack": "^1.9.3",
    "@types/chai": "^4.1.2",
    "@types/mocha": "^5.2.5",
    "@types/node": "9.3.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.2.0",
    "copy-webpack-plugin": "^4.1.1",
    "exports-loader": "^0.6.3",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^2.29.0",
    "mocha": "^5.2.0",
    "morphi": "2.1.1",
    "ngc-webpack": "^4.1.2",
    "reflect-metadata": "^0.1.10",
    "ts-loader": "^2.3.1",
    "ts-node": "^7.0.1",
    "tslib": "^1.8.1",
    "tslint": "^5.9.1",
    "typescript": "2.6.2",
    "uglifyjs-webpack-plugin": "~1.1.2",
    "webpack": "3.10.0",
    "webpack-dev-server": "~2.9.3"
  },
  "license": "MIT",
  "private": false
}

{
  "name": "ng2-logger",
  "version": "2.0.35",
  "description": "isomorphic logger for browser/server in typescript",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/darekf77/ng2-logger.git"
  },
  "tnp": {
    "type": "isomorphic-lib",
    "resources": [
      "README.md",
      "screen.png",
      "server.png"
    ]
  },
  "browser": {
    "process": false
  },
  "keywords": [
    "isomorphic",
    "logger",
    "log",
    "typescript"
  ],
  "author": {
    "name": "Dariusz Filipiak"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/darekf77/ng2-logger/issues"
  },
  "homepage": "https://github.com/darekf77/ng2-logger#readme",
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "tsc -p ./ --outDir dist/"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "readmeFilename": "README.md",
  "dependencies": {
    "chalk": "^2.3.0",
    "json-stringify-safe": "^5.0.1",
    "json5": "^0.5.1",
    "randomcolor": "^0.5.3"
  },
  "devDependencies": {
    "tslib": "^1.9.0",
    "typescript": "^2.6.2"
  }
}

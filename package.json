{
  "tnp": {
    "resources": [
      "README.md",
      "screen.png",
      "server.png"
    ],
    "type": "isomorphic-lib",
    "overrided": {
      "includeAsDev": [
        "morphi",
        "@angular.*",
        "webpack",
        "webpack-dev-server",
        "html-webpack-plugin",
        "ts-loader",
        "babel.*",
        "mocha",
        "@types/mocha",
        "chai",
        "ts-sinon"
      ],
      "includeOnly": [
        "chalk",
        "json-stringify-safe",
        "json5",
        "randomcolor",
        "enum-values"
      ],
      "dependencies": {
        "enum-values": "^1.2.1"
      }
    }
  },
  "name": "ng2-logger",
  "version": "2.0.47",
  "description": "isomorphic logger for browser/server in typescript",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/darekf77/ng2-logger.git"
  },
  "browser": {
    "process": false
  },
  "keywords": [
    "isomorphic",
    "logger",
    "logging",
    "logowanie",
    "log",
    "typescript"
  ],
  "author": {
    "name": "Dariusz Filipiak"
  },
  "bugs": {
    "url": "https://github.com/darekf77/ng2-logger/issues"
  },
  "homepage": "https://github.com/darekf77/ng2-logger#readme",
  "readmeFilename": "README.md",
  "husky": {
    "hooks": {
      "pre-push": "tnp deps:show:if:standalone"
    }
  }
}
